<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>


<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.TextArea?>
<DialogPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ticket.TicketDialogController">
   <content>
      <GridPane>
         <Label text="Ticket Title"/>
         <TextField fx:id="ticketTitleField" GridPane.rowIndex="1"/>
         <Label text="Ticket Details" GridPane.rowIndex="2"/>
         <TextArea fx:id="ticketDetailsArea" GridPane.rowIndex="3" wrapText="true"/>
      </GridPane>
   </content>
</DialogPane>
